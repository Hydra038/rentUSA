QUICKSTART.md
BUILD.md
DEPLOYMENT.md
SUPABASE-VERCEL-SETUP.md

# Development files
*.md
.vscode
.idea

# Environment files (use Vercel dashboard for env vars)
.env
.env.local
.env.development
.env.test
.env.production

# Git
.git
.gitignore

# Dependencies (Vercel installs these)
node_modules

# Build outputs (Vercel generates these)
.next
out
dist

# Test files
**/*.test.ts
**/*.test.tsx
**/*.spec.ts
**/*.spec.tsx
__tests__
coverage

# Logs
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# OS files
.DS_Store
Thumbs.db
